{
  "name": "@rynpsc/dialog",
  "version": "3.0.0",
  "description": "A modern, lightweight and accessible dialog with a focus on progressive enhancement",
  "keywords": [
    "accessibility",
    "accessible",
    "dialog",
    "modal"
  ],
  "homepage": "https://github.com/rynpsc/dialog",
  "bugs": "https://github.com/rynpsc/dialog/issues",
  "repository": "github:rynpsc/dialog",
  "license": "MIT",
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/dialog.js",
  "unpkg": "dist/dialog.umd.js",
  "module": "dist/dialog.mjs",
  "scripts": {
    "build": "rimraf dist && microbundle build src/index.js --external none",
    "dev": "microbundle watch src/index.js --external none",
    "prepare": "npm run -s build"
  },
  "dependencies": {
    "@rynpsc/focus-trap": "^1.0.1"
  },
  "devDependencies": {
    "microbundle": "^0.11.0",
    "rimraf": "^2.6.3"
  }
}
